    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["default.json", "raven"],
            "sudo": true
        },
        {
            "name": "Attach to Mono",
            "request": "attach",
            "type": "mono",
            "address": "10.100.0.105",
            "port": 8000
        }
    ]
}